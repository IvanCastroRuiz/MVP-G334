#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/proc/cygdrive/e/Profe/MVP G334/node_modules/.pnpm/ts-jest@29.4.4_@babel+core@_20d7d436a676602aa0dbfc304596ec78/node_modules/ts-jest/node_modules:/proc/cygdrive/e/Profe/MVP G334/node_modules/.pnpm/ts-jest@29.4.4_@babel+core@_20d7d436a676602aa0dbfc304596ec78/node_modules:/proc/cygdrive/e/Profe/MVP G334/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/proc/cygdrive/e/Profe/MVP G334/node_modules/.pnpm/ts-jest@29.4.4_@babel+core@_20d7d436a676602aa0dbfc304596ec78/node_modules/ts-jest/node_modules:/proc/cygdrive/e/Profe/MVP G334/node_modules/.pnpm/ts-jest@29.4.4_@babel+core@_20d7d436a676602aa0dbfc304596ec78/node_modules:/proc/cygdrive/e/Profe/MVP G334/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../cli.js" "$@"
else
  exec node  "$basedir/../../cli.js" "$@"
fi
