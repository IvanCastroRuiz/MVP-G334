#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/proc/cygdrive/e/Profe/MVP G334/node_modules/.pnpm/typeorm@0.3.27_pg@8.16.3_re_7efd3ab46f3452adc47d54828e08126f/node_modules/typeorm/node_modules:/proc/cygdrive/e/Profe/MVP G334/node_modules/.pnpm/typeorm@0.3.27_pg@8.16.3_re_7efd3ab46f3452adc47d54828e08126f/node_modules:/proc/cygdrive/e/Profe/MVP G334/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/proc/cygdrive/e/Profe/MVP G334/node_modules/.pnpm/typeorm@0.3.27_pg@8.16.3_re_7efd3ab46f3452adc47d54828e08126f/node_modules/typeorm/node_modules:/proc/cygdrive/e/Profe/MVP G334/node_modules/.pnpm/typeorm@0.3.27_pg@8.16.3_re_7efd3ab46f3452adc47d54828e08126f/node_modules:/proc/cygdrive/e/Profe/MVP G334/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../typeorm/cli-ts-node-commonjs.js" "$@"
else
  exec node  "$basedir/../typeorm/cli-ts-node-commonjs.js" "$@"
fi
